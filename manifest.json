{
  "manifest_version": 2,
  "name": "Twitter Cleaner",
  "version": "1.0",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.twitter.com/*"
      ],
      "js": [
        "dist/inject.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "dist/script.js"
  ]
}
